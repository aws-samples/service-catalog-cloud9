{
    "DefaultParameterGroup": "Tags",
    "Configuration": [
        {
            "Key": "App",
            "Value": "${Application}",
            "Default": "Cloud9",
            "Description": "The name of the application"
        },
        {
            "Key": "Owner",
            "Value": "${Owner}",
            "Default": "AWS-Samples",
            "Description": "The owner of the product (Free Text)",
            "CreateTagOption":false
        },
        {
            "Key": "CostCtrr",
            "Value": "${CostCenter}",
            "Default": "001"
        },
        {
            "Key": "Env",
            "Value": "${Environment}",
            "Default": "DEV",
            "AllowedValues": [
                "DEV",
                "QA",
                "UAT",
                "PROD"
            ]
        }
    ]
}